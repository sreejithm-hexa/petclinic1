@Library("Shared-Library-Sreejith")_
import com.app.MavenUtil

def utils = new MavenUtil(this)

pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                script {
                    utils.mvn 'compile'
                }
            }
        }
        stage('Test') {
            steps {
                script {
                    utils.mvn 'test'
                }
            }
        }
        stage('Package') {
            steps {
                script {
                    utils.mvn 'clean package'
                }
            }
        }
        stage('liquibase Test') {
            steps {               
                    sh '/opt/liquibase/liquibase --version'             
            }
        }
    }
}
